{{#if emptySnippet}}
<div class="row">
  <div class="col s12">
    You must provide a snippet parameter
  </div>
</div>
{{else}}
<div class="mdl-grid">
  <div class={{exampleFrameClass}}>
    {{partial partialName}}
  </div>
</div>
<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--12-col">
    {{code-snippet name=snippetName}}
  </div>
</div>
{{/if}}